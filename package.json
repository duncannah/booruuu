{
	"name": "booruuu",
	"version": "0.0.1",
	"scripts": {
		"client": "cd client && yarn-or-npm run start",
		"start": "nodemon --watch server --watch node_modules ./server",
		"dev": "concurrently --kill-others-on-fail \"yarn-or-npm run start\" \"yarn-or-npm run client\""
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"classnames": "^2.2.6",
		"express": "^4.16.4",
		"fs-extra": "^7.0.1",
		"node-fetch": "^2.6.1",
		"npm-run-all": "^4.1.5"
	},
	"devDependencies": {
		"concurrently": "^3.5.0",
		"nodemon": "^1.19.2",
		"yarn-or-npm": "^3.0.1"
	},
	"repository": "duncannah/booruuu",
